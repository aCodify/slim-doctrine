{
    "name": "juliangut/slim-doctrine",
    "description": "Slim Framework Doctrine integration",
    "keywords": [
        "slim",
        "framework",
        "doctrine",
        "handler"
    ],
    "homepage": "http://github.com/juliangut/slim-doctrine",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Julián Gutiérrez",
            "email": "juliangut@gmail.com",
            "homepage": "http://juliangut.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "slim/slim": "3.0.0-RC2",
        "doctrine/orm": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "squizlabs/php_codesniffer": "~2",
        "phpmd/phpmd": "~2.2",
        "sebastian/phpcpd": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Jgut\\Slim\\Doctrine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jgut\\Slim\\Doctrine\\Tests\\": "tests/Doctrine/"
        }
    }
}
